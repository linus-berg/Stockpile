{
  "db_path": "./Storage/",
  "sql_path": "./Sql/",
  "delta_format": "yyyyMMdd",
  "filters": {
    "npm-filter": {
      "@graphql-codegen": {
        "version": "alpha"
      },
      "typescript": {
        "version": "dev"
      },
      "google-closure-compiler": {
        "version": "nightly"
      }
    }
  },
  "fetchers": [
    {
      "id": "maven",
      "type": "maven",
      "filters": [],
      "threading": {
        "parallel_pkg": 6,
        "parallel_ver": 1
      },
      "input": "./Input/MAVEN.txt",
      "output": {
        "full": "../PACKAGES/MAVEN/",
        "delta": "../DELTA/MAVEN/"
      }
    },
    {
      "id": "npm",
      "type": "npm",
      "filters": [
        "npm-filter"
      ],
      "threading": {
        "parallel_pkg": 6,
        "parallel_ver": 1
      },
      "input": "./Input/NPM.txt",
      "output": {
        "full": "../PACKAGES/NPM/",
        "delta": "../DELTA/NPM/"
      }
    },
    {
      "id": "git",
      "type": "git",
      "threading": {
        "parallel_pkg": 6,
        "parallel_ver": 1
      },
      "input": "./Input/GIT.txt",
      "output": {
        "full": "../PACKAGES/GIT/",
        "delta": "../DELTA/GIT/"
      }
    },
    {
      "id": "nuget",
      "type": "nuget",
      "threading": {
        "parallel_pkg": 6,
        "parallel_ver": 1
      },
      "input": "./Input/NUGET.txt",
      "output": {
        "full": "../PACKAGES/NUGET/",
        "delta": "../DELTA/NUGET/"
      }
    }
  ]
}
