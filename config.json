{
  "db_path": "./Storage/",
  "delta_format": "yyyyMMdd",
  "filters": {
    "npm-filter": {
      "@graphql-codegen": {
        "version": "alpha"
      },
      "typescript": {
        "version": "dev"
      },
      "google-closure-compiler": {
        "version": "nightly"
      }
    }
  },
  "channels": [
    {
      "id": "maven",
      "type": "maven",
      "force": true,
      "filters": [],
      "threads": {
        "parallel_pkg": 6,
        "parallel_ver": 1
      },
      "input": "./Input/MAVEN.txt",
      "output": {
        "full": "./Output/maven/full/",
        "delta": "./Output/maven/delta/"
      }
    },
    {
      "id": "npm",
      "type": "npm",
      "force": false,
      "filters": [
        "npm-filter"
      ],
      "options": "get_peers;",
      "threads": {
        "parallel_pkg": 6,
        "parallel_ver": 1
      },
      "input": "./Input/NPM.txt",
      "output": {
        "full": "./Output/npm/full/",
        "delta": "./Output/npm/delta/"
      }
    },
    {
      "id": "git",
      "type": "git",
      "force": false,
      "threads": {
        "parallel_pkg": 6,
        "parallel_ver": 1
      },
      "input": "./Input/GIT.txt",
      "output": {
        "full": "./Output/git/full/",
        "delta": "./Output/git/delta/"
      }
    },
    {
      "id": "nuget",
      "type": "nuget",
      "force": false,
      "threads": {
        "parallel_pkg": 6,
        "parallel_ver": 1
      },
      "input": "./Input/NUGET.txt",
      "output": {
        "full": "./Output/nuget/full/",
        "delta": "./Output/nuget/delta/"
      }
    }
  ]
}
