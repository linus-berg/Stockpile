{
  "db_path": "./Store/",
  "filters": {
    "npm-filter": {
      "*": {
        "version": "dev"
      },
      "@graphql-codegen": {
        "version": "alpha"
      },
      "typescript": {
        "version": "dev"
      },
      "google-closure-compiler": {
        "version": "nightly"
      }
    }
  },
  "fetchers": [
    {
      "id": "npm",
      "type": "npm",
      "filters": [
        "npm-filter"
      ],
      "threading": {
        "parallel_pkg": 6,
        "parallel_ver": 1
      },
      "input": "./NPM.txt",
      "output": {
        "full": "./PACKAGES/NPM/",
        "delta": "./DELTA/NPM/"
      }
    },
    {
      "id": "git",
      "type": "git",
      "threading": {
        "parallel_pkg": 6,
        "parallel_ver": 1
      },
      "input": "./GIT.txt",
      "output": {
        "full": "./PACKAGES/GIT/",
        "delta": "./DELTA/GIT/"
      }
    },
    {
      "id": "nuget",
      "type": "nuget",
      "threading": {
        "parallel_pkg": 6,
        "parallel_ver": 1
      },
      "input": "./NUGET.txt",
      "output": {
        "full": "./PACKAGES/NUGET/",
        "delta": "./DELTA/NUGET/"
      }
    }
  ]
}
