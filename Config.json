{
  "db_path": "./Store/",
  "fetchers": [
    {
      "id": "npm",
      "type": "npm",
      "threading": {
        "parallel_pkg": 6,
        "parallel_ver": 1
      },
      "input": "./NPM.txt",
      "output": {
        "full": "./PACKAGES/NPM/",
        "delta": "./DELTA/NPM/"
      }
    },
    {
      "id": "git",
      "type": "git",
      "threading": {
        "parallel_pkg": 6,
        "parallel_ver": 1
      },
      "input": "./GIT.txt",
      "output": {
        "full": "./PACKAGES/GIT/",
        "delta": "./DELTA/GIT/"
      }
    },
    {
      "id": "nuget",
      "type": "nuget",
      "threading": {
        "parallel_pkg": 6,
        "parallel_ver": 1
      },
      "input": "./NUGET.txt",
      "output": {
        "full": "./PACKAGES/NUGET/",
        "delta": "./DELTA/NUGET/"
      }
    }
  ]
}
